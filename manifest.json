{
    "manifest_version" : 3,
    "name" : "TimeTracker" ,
    "description" : "Tracking daily time spend on websites" ,
    "permissions" : ["tabs" , "storage"],
    "host_permissions": ["<all_urls>"],
    "version" : "1.0.0",
    "background" : {
        "service_worker" : "background.js"
    },
    "action" : {
        "default_popup" : "popup.html" ,
        "default_icon" : "icon.png"
    }
}